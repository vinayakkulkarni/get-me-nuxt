{
  "name": "get-me-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "click-outside-vue3": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^17.7.0",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@nuxt/devtools": "^1.0.1",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.17.0",
    "@unocss/eslint-config": "^0.57.3",
    "@unocss/nuxt": "^0.56.5",
    "@unocss/preset-wind": "^0.57.3",
    "@unocss/reset": "^0.57.3",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.4",
    "prettier": "^3.0.3",
    "sass": "^1.69.6",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.2.2",
    "unocss-preset-scrollbar": "^0.3.0"
  },
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "url": "git+https://github.com/vinayakkulkarni/get-me-nuxt.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/get-me-nuxt/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/get-me-nuxt"
}
